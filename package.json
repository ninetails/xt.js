{
  "name": "@ninetails/xt",
  "version": "0.0.1",
  "description": "Tiny view template library inspired on https://gist.github.com/plugnburn/07c383da5f151a54d0b2",
  "main": "index.js",
  "jsnext:main": "index.mjs",
  "browser": "index.browser.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run rollup && npm run babel",
    "rollup": "node -r esm build.js",
    "babel": "babel src --out-dir dist/es",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "eslint": "eslint src",
    "precommit": "npm run eslint",
    "prepush": "npm run test:coverage -- --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninetails/xt.js.git"
  },
